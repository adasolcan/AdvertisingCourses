<h1>Listing course_enrolments</h1>

<table class="table table-striped" style="margin-top:10px; float:left;">
  <thead>
    <tr>
        <th>Prenume</th>
        <th>Nume</th>
        <th>Session</th>
        <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_enrolments.each do |course_enrolment| %>
      <tr>
        <td> <%= User.find_by_id(course_enrolment.user_id).first_name %> </td>
        <td> <%= User.find_by_id(course_enrolment.user_id).last_name %></td>
        <td><%= course_enrolment.session_id %></td>
        <td><%= link_to 'Show', course_enrolment %></td>
        <td><%= link_to 'Edit', edit_course_enrolment_path(course_enrolment) %></td>
        <td><%= link_to 'Destroy', course_enrolment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course enrolment', new_course_enrolment_path %>
