<%= form_for(@course_session) do |f| %>
  <% if @course_session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@course_session.errors.count, "error") %> prohibited this course_session from being saved:</h2>

      <ul>
      <% @course_session.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :denumire_sesiune %><br> 
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :nume_curs %><br>
    <%= f.collection_select :course_id, Course.order(:title),:id,:title, include_blank: true %>
  </div>
  <div class="field">
    <%= f.label :email_instructor %><br>
    <%= f.collection_select :instructor_id, User.order(:email),:id,:email, include_blank: true %>
  </div>
  <div class="field">
    <%= f.label :data_si_ora %><br>
    <%= f.datetime_select :date_time %>
  </div>
  <div class="field">
    <%= f.label :locul %><br>
    <%= f.text_field :place %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
